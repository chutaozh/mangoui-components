{
  "name": "@mangoui/components",
  "version": "0.0.0-alpha.1",
  "description": "A simple library of React UI components",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/chutaozh/mangoui-components.git"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve -c scripts/webpack.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack -c scripts/webpack.config.js --mode production",
    "rm": "node scripts/fs.config.js rm",
    "cp": "node scripts/fs.config.js cp",
    "tsc": "npm run rm && tsc --project tsconfig.build.json && npm run cp"
  },
  "keywords": [
    "mangoui",
    "react",
    "components"
  ],
  "author": "ChuTao Zhang",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "classnames": "^2.3.2",
    "core-js": "^3.32.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpackbar": "^5.0.2"
  }
}